{
  "env": {
    "browser": true
  },
  "globals": {
    "define": true,
    "module": true,
    "require": true,
    "exports": true
  },
  "rules": {
    "no-bitwise": 2,
    "camelcase": 2,
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "no-new": 2,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "curly": 2,
    "eqeqeq": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-caller": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-unused-expressions": 2,
    "no-loop-func": 2
  }
}
